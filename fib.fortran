C FORTRAN IV PROGRAM TO LIST FIRST 20 FIBONACCI NUMBERS
      INTEGER F1, F2, F3, I
      F1 = 0
      F2 = 1
      WRITE (6, 10) F1
   10 FORMAT (I10)
      WRITE (6, 10) F2
      DO 20 I = 3, 20
      F3 = F1 + F2
      WRITE (6, 10) F3
      F1 = F2
      F2 = F3
   20 CONTINUE
      STOP
      END
